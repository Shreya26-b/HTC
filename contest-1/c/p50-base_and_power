#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int i, a, b, pdt=1;
    scanf("%d %d", &a, &b);
    for(i=1; i<=b; ++i) 
        pdt*=a;
    printf("%d", pdt);
    return 0;
}
