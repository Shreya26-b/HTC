#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int m, n, a[100][100], i, j;
    scanf("%d %d", &m, &n);
    for(i=0; i<m; ++i) {
        for(j=0; j<n; ++j)
            scanf("%d", &a[i][j]);
    }
    printf("%d %d %d %d", a[0][0], a[0][n-1], a[m-1][0], a[m-1][n-1]);
    return 0;
}
