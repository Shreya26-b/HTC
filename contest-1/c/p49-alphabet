#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    char c;
    scanf("%c", &c);
    if((c>=65 && c<=90) || (c>=97 && c<=122)) 
        printf("Yes");
    else 
        printf("No");
    return 0;
}
